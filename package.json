{
  "name": "@s21toolkit/introspector",
  "description": "GQL Introspection CLI for edu.21-school.ru",
  "type": "module",
  "bin": {
    "s21i": "build/main.js"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "lint:tsc": "tsc",
    "lint:biome": "biome check .",
    "lint": "pnpm lint:biome && pnpm lint:tsc",
    "fix:biome": "biome check --write .",
    "fix": "pnpm fix:biome",
    "build:tsup": "tsup",
    "build": "pnpm lint && pnpm build:tsup",
    "postversion": "git push && git push --tags",
    "release": "npm version -m 'chore(release): %s' --",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@s21toolkit/shared": "^1.4.8",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/common-tags": "^1.8.4",
    "@types/fs-extra": "^11.0.4",
    "@types/har-format": "^1.2.16",
    "@types/node": "^24.10.4",
    "tsup": "8.4.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@s21toolkit/auth": "^1.2.1",
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "cmd-ts": "^0.14.3",
    "common-tags": "^1.8.2",
    "fs-extra": "^11.3.3",
    "graphql": "^16.12.0",
    "node-html-parser": "^7.0.1",
    "ts-pattern": "^5.9.0"
  },
  "private": false,
  "license": "MIT",
  "version": "1.1.1",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s21toolkit/s21introspector.git"
  }
}
